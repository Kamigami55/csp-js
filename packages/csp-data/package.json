{
  "name": "@csp-js/data",
  "version": "0.1.0",
  "description": "Service definitions and CSP mappings database for csp-js",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./services": {
      "types": "./dist/services.d.ts",
      "import": "./dist/services.js",
      "require": "./dist/services.cjs"
    }
  },
  "files": [
    "dist",
    "data",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "validate": "node scripts/validate-data.js",
    "prepare": "npm run build",
    "prepack": "npm run build && npm run validate",
    "prepublishOnly": "npm run lint && npm run check-types"
  },
  "keywords": [
    "csp",
    "content-security-policy",
    "services",
    "data",
    "mappings"
  ],
  "author": "Eason",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/easonz/csp-js.git",
    "directory": "packages/csp-data"
  },
  "bugs": {
    "url": "https://github.com/easonz/csp-js/issues"
  },
  "homepage": "https://csp-js.dev",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/easonz"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.29.0",
    "tsup": "^8.0.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  }
}